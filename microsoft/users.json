          {
            "oauth2": ["User.Read", "User.Read.All", "User.ReadWrite", "User.ReadWrite.All", "Directory.Read.All", "Directory.ReadWrite.All"]
          }
        ]
      },
      "patch": {
        "summary": "Update user",
        "description": "Update the properties of a user object",
        "operationId": "updateUser",

          {
            "name": "user-id",
            "in": "path",
            "description": "The unique identifier of the user. Can be the id property or userPrincipalName",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "User properties to update",
          "required": true,
          "content": {

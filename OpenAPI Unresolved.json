    "/calendars/{calendarId}/events": {
      "get": {
        "summary": "List events",
        "operationId": "listEvents",
        "tags": ["Events"],
        "parameters": [
          {
            "$ref": "#/components/parameters/CalendarId"
          },
          {
            "name": "maxResults",
            "in": "query",
            "description": "Maximum number of events returned",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 2500,
              "default": 250
            }
          },
          {
            "name": "pageToken",
            "in": "query",
            "description": "Token for pagination",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "timeMin",
            "in": "query",
            "description": "Lower bound for event start time (RFC3339)",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "timeMax",
            "in": "query",
            "description": "Upper bound for event start time (RFC3339)",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "q",
            "in": "query",
            "description": "Free text search terms",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "orderBy",
            "in": "query",
            "description": "Order of events returned",
            "schema": {
              "type": "string",
              "enum": ["startTime", "updated"]
            }
          },
          {
            "name": "singleEvents",
            "in": "query",
            "description": "Whether to expand recurring events",
            "schema": {
              "type": "boolean",
              "default": false
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of events",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Events"
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
